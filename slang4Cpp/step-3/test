          else if (isalnum(exp[index] == true)){
               int strStartIndex = index;
               while (index < length &&
                      (isalnum(exp[index]) || exp[index] == '_')) {
                    index++;
               }
               size_t lenOfStr = index - strStartIndex ;
               std::strncpy(last_string, exp+strStartIndex, lenOfStr);
               tok = TOK_UNQUOTED_STRING;
               